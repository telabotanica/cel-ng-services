# This file is a "template" of which env vars need to be defined for your application
# Copy this file to .env file for development, create environment variables when deploying to production
# https://symfony.com/doc/current/best_practices/configuration.html#infrastructure-related-configuration

###> symfony/framework-bundle ###
APP_ENV=dev
APP_SECRET=XXXXXXXXXXXXXXXXXXXXXXXXXXX
#TRUSTED_PROXIES=127.0.0.1,127.0.0.2
#TRUSTED_HOSTS=localhost,example.com
###< symfony/framework-bundle ###

#The base relative path the app is installed at
APP_PREFIX_PATH=/cel2-dev/cel2-services/public

###> nelmio/cors-bundle ###
CORS_ALLOW_ORIGIN=^https?://localhost:?[0-9]*$
###< nelmio/cors-bundle ###

###> doctrine/doctrine-bundle ###
# Format described at http://docs.doctrine-project.org/projects/doctrine-dbal/en/latest/reference/configuration.html#connecting-using-a-url
# For an SQLite database, use: "sqlite:///%kernel.project_dir%/var/data.db"
# Configure your db driver and server_version in config/packages/doctrine.yaml
DATABASE_URL=mysql://testsf-dev:testsf-dev@127.0.0.1:3306/testsf-dev
###< doctrine/doctrine-bundle ###

# The base URL for elasticsearch index used by our app
# CAUTION! this is internal. You still must configure elastica in its own conf
ELASTICSEARCH_INDEX_URL=http://localhost:9200/cel2_occurrences/

###> Folder where generated files are stored ###
TMP_FOLDER=/tmp

###> lexik/jwt-authentication-bundle ###
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=XXXXXXXXXXXXXXXXXXXXXXXXXXXXXXX
###< lexik/jwt-authentication-bundle ###
