{
    "name": "telabotanica/cel-ng-services",
    "description": "Tela Botanica - Carnet en Ligne - services",
    "type": "project",
    "license": "GPLv3",
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "algolia/search-bundle": "3.4",
        "api-platform/core": "^2.1",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.4.5",
        "fresh/vich-uploader-serialization-bundle": "~2.1",
        "friendsofsymfony/elastica-bundle": "^5.0",
        "liip/imagine-bundle": "^2.0",
        "nelmio/cors-bundle": "^1.5",
        "phpdocumentor/reflection-docblock": "^3.0 || ^4.0",
        "phpoffice/phpspreadsheet": "^1.5",
        "sensio/framework-extra-bundle": "^5.2",
        "symfony/asset": "^3.0 || ^4.0",
        "symfony/cache": "4.3.*",
        "symfony/console": " 4.3.*",
        "symfony/debug": "4.3.*",
        "symfony/debug-bundle": "4.3.*",
        "symfony/dotenv": "4.3.*",
        "symfony/expression-language": "^3.0 || ^4.0",
        "symfony/flex": "^1.12",
        "symfony/form": "4.3.*",
        "symfony/framework-bundle": "4.3.*",
        "symfony/http-foundation": "4.3.*",
        "symfony/monolog-bundle": "^3.3",
        "symfony/security-bundle": "^3.0 || ^4.0",
        "symfony/security-guard": "4.3.*",
        "symfony/serializer": "4.3.*",
        "symfony/translation": "4.3.*",
        "symfony/twig-bundle": "^3.0 || ^4.0",
        "symfony/validator": "^3.0 || ^4.0",
        "symfony/var-dumper": "4.3.*",
        "symfony/web-server-bundle": "4.3.*",
        "symfony/yaml": "4.3.*",
        "tecnickcom/tcpdf": "^6.2",
        "vich/uploader-bundle": "^1.8"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "easycorp/easy-deploy-bundle": "^1.0",
        "justinrainbow/json-schema": "^5.2",
        "symfony/browser-kit": "^4.3",
        "symfony/css-selector": "^4.3",
        "symfony/debug-pack": "^1.0",
        "symfony/http-client": "4.3.*",
        "symfony/maker-bundle": "^1.5",
        "symfony/phpunit-bridge": "^5.2"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.3.*"
        }
    }
}
